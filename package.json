{
  "name": "@eggproject/cypress-xhr-recorder",
  "version": "0.0.2",
  "description": "Cypress xhr recorder and player",
  "main": "dist",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "=10.16.3"
  },
  "files": [
    "dist/",
    "src/",
    "package-lock.json",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "dev": "tsc --declarationMap -w",
    "prepublish": "tsc --declarationMap",
    "cypress:playground:record:open": "npm run prepublish && cypress open --project ./playground --env xhr_recorder_recordMode=true",
    "cypress:playground:record:run": "npm run prepublish && cypress run --project ./playground --env xhr_recorder_recordMode=true",
    "cypress:playground:play:open": "npm run prepublish && cypress open --project ./playground --env xhr_recorder_recordMode=false",
    "cypress:playground:play:run": "npm run prepublish && cypress run --project ./playground --env xhr_recorder_recordMode=false",
    "test:backend:start": "node test/test-backend.js",
    "test:record:frontend:start": "http-server test -p 4200",
    "record:start": "node dist/node/record.js",
    "prerecord:start": "npm run prepublish",
    "play:start": "node dist/node/play.js",
    "preplay:start": "npm run prepublish",
    "cypress:playground:record:frontend:start": "start-test test:record:frontend:start http://localhost:4200 test:backend:start",
    "cypress:playground:record:backend:start": "start-test cypress:playground:record:frontend:start http://localhost:8000 record:start",
    "cypress:playground:play:backend:start": "start-test test:record:frontend:start http://localhost:4200 play:start",
    "cypress:playground:record:open:start": "start-test cypress:playground:record:backend:start http://localhost:9000 cypress:playground:record:open",
    "cypress:playground:record:shell:start": "start-test cypress:playground:record:backend:start http://localhost:9000 cypress:playground:record:run",
    "cypress:playground:play:open:start": "start-test cypress:playground:play:backend:start http://localhost:9000/__wait cypress:playground:play:open",
    "cypress:playground:play:shell:start": "start-test cypress:playground:play:backend:start http://localhost:9000/__wait cypress:playground:play:run",
    "test": "npm run cypress:playground:record:shell:start && npm run cypress:playground:play:shell:start"
  },
  "author": "eggp",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EggProject/cypress-xhr-recorder.git"
  },
  "keywords": [
    "cypress",
    "command",
    "xhr",
    "record",
    "play"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EggProject/cypress-xhr-recorder/issues"
  },
  "homepage": "https://github.com/EggProject/cypress-xhr-recorder#readme",
  "peerDependencies": {
    "cypress": "3.0.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "5.4.4",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.7",
    "@types/node": "12.12.21",
    "cors": "2.8.5",
    "cypress": "5.0.0",
    "http-server": "0.12.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "ntl": "5.1.0",
    "prettier": "2.0.5",
    "rxjs": "6.6.2",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.3",
    "ts-loader": "8.0.2",
    "typescript": "3.6.5",
    "webpack": "4.41.4"
  },
  "dependencies": {
    "@angular/cdk": "10.1.3",
    "@angular/core": "10.0.11",
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "debug": "4.1.1",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "load-json-file": "6.2.0"
  }
}
